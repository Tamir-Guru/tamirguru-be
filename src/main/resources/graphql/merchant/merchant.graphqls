scalar Long

type Merchant {
    merchantName: String
    photo: String
    email: String
    details: String
    id: ID
    createDate: Date
    phoneNumber: String
    fullAddress: String
    website: String
    countryPhoneCode: String
    countryName: String
    cityName: String
    regionName: String
    districtName: String
    longitude: Float
    latitude: Float
    merchantTypes: [String]
    features: [MerchantFeature]
}

input MerchantFilter {
    types: [String]
    districts: [Int]
    cities: [String]
    features: [String]
    values: [Int]
    distance: Distance
}

input Distance {
    longitude: Float!
    latitude: Float!
    distance: Long!
}

type Feature {
    feature: String!
    values: [FeatureValue]!
}

type FeatureValue {
    name: String!
    value: Int!
}

type MerchantFeature {
    featureSet: [Feature]!
    merchantType: String!
    merchantTypeId: String!
}