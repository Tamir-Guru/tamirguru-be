scalar Upload
scalar Date

type User {
    email: String
    name: String
    surname: String
    role: String
    photo: String
    token: String
    verified: Boolean
    createDate: Date
}

input UserCreate {
    birthdate: Date
    email: String!
    name: String!
    password: String!
    role: Role!
    surname: String!
    tckn: String
}

input UserUpdate {
    email: String!
    name: String!
    surname: String!
}
